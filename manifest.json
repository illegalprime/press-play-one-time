{
    "name": "Getting Started Example",
    "version": "1.0",
    "description": "Build an Extension!",
    "manifest_version": 2,
    "browser_action": {
        "default_title": "Kastt",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*", "file://*/*"],
            "js": ["ServerDate.js", "content.js"]
        }
    ],
    "permissions": ["<all_urls>", "activeTab"]
}
